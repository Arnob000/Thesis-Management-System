doctype html
head
  meta(charset='UTF-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title ResearhNet
  style
         include ../static/reStu.css
  link(rel='stylesheet' href='https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css')
section.header
  nav.navP
    a(href='home.html')
      img(src='images/logo.png')
    .nav-links
      ul
        li
          a(href='/home') HOME
        li
          a(href='/profile') PROFILE
        li
          a(href='/reinfoStu') RESEARCH INFO
        li
          a(href='/newProject') NEW PROJECT
        li
          a(href='QA') Q&amp;A
        li
          a(href='/') LOG OUT
    div
      ul
        li
          a(href='/contact2') CONTACT US 
section.review
  .r2
    h1 Research Information
    div.c2
        |#{result}
    .rRow
      .rCol
        div          
          p
          div.info
            ul 
              each item in items 
                div.ele
                    li #[a(href='/newShow2/'+item[0]) #{item[1]} ]
                    h4 Researcher Name:  #{item[4]}  #{item[5]}
                    h4 Email: #{item[5]}
                    h4 Designation: #{item[6]}
                    h5 Description: 
                    h5 #{item[2]}
                                       
            

section.footer
  h4 About Us
  p
    | This website was created to serve all the research enthusiastics around the world.
    br
    | We hope together we can make the world a better place.
  .icons
    i.fa.fa-facebook
    i.fa.fa-twitter
    i.fa.fa-instagram
    i.fa.fa-linkedin






       
    
   
